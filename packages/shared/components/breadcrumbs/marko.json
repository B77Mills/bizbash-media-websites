{
  "<global-breadcrumbs>": {
    "template": "./index.marko",
    "@items <item>[]": {}
  },
  "<global-breadcrumbs-website-section>": {
    "template": "./website-section.marko",
    "@section": "object"
  },
  "<global-breadcrumbs-magazine>": {
    "template": "./magazine.marko",
    "@target": "object"
  },
  "<global-breadcrumbs-dynamic-page>": {
    "template": "./dynamic-page.marko",
    "@name": {
      "type": "string",
      "required": true
    },
    "<global-breadcrumbs-published-content>": {
      "template": "./published-content.marko",
      "@title": {
        "type": "string",
        "required": true
      }
    },
    "<global-breadcrumbs-search>": {
      "template": "./search.marko"
    },
    "<global-breadcrumbs-login>": {
      "template": "./login.marko"
    },
    "<global-breadcrumbs-subscribe>": {
      "template": "./subscribe.marko",
      "@value": "string"
    }
  }
}
