  },
  "<global-magazine-issue-archive-node>": {
    "template": "./magazine-issue-archive.marko",
    "@node": "object",
    "@image-width": "number",
    "@full-height": "boolean",
    "@flush": "boolean",
    "@card": "boolean",
    "@with-title": "boolean"
  },
  "<global-contact-us-list-node>": {
    "template": "./contact-us-list.marko",
    "@node": "object"
  }
