{
  "<global-author-card-node>": {
    "template": "./author-card.marko",
    "@node": "object",
    "@with-email": "boolean"
  },
  "<global-content-list-node>": {
    "template": "./content-list.marko",
    "@node": "object",
    "@display-image": {
      "type": "boolean",
      "default-value": true
    },
    "@with-teaser": {
      "type": "boolean",
      "default-value": false
    },
    "@attrs": "object",
    "@link-attrs": "object",
    "@image-width": {
      "type": "number",
      "default-value": 126
    },
    "@image-ar": {
      "type": "string",
      "default-value": "3:2"
    },
    "@image-position": {
      "type": "string",
      "default-value": "left"
    }
  },
  "<global-content-card-node>": {
    "template": "./content-card.marko",
    "@node": "object",
    "@title-modifiers": "array",
    "@image-width": {
      "type": "number",
      "default-value": 300
    },
    "@image-ar": {
      "type": "string",
      "default-value": "3:2"
    },
    "@display-image": {
      "type": "boolean",
      "default-value": true
    },
    "@with-teaser": {
      "type": "boolean",
      "default-value": true
    },
    "@attrs": "object",
    "@link-attrs": "object"
  },
  "<global-content-hero-card-node>": {
    "template": "./content-hero-card.marko",
    "@node": "object",
    "@modifiers": "array",
    "@title-modifiers": "array",
    "@image-width": {
      "type": "number",
      "default-value": 300
    },
    "@image-ar": {
      "type": "string",
      "default-value": "6:5"
    },
    "@display-image": {
      "type": "boolean",
      "default-value": true
    },
    "@with-teaser": {
      "type": "boolean",
      "default-value": true
    },
    "@attrs": "object",
    "@link-attrs": "object"
  },
  "<global-featured-products-card-node>": {
    "template": "./featured-products-card.marko",
    "@node": "object",
    "@image-width": {
      "type": "number",
      "default-value": 300
    },
    "@with-teaser": {
      "type": "boolean",
      "default-value": true
    },
    "@link-attrs": "object"
  },
  "<global-youtube-card-node>": {
    "template": "./youtube-card.marko",
